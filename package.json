{
    "name": "franky",
    "displayName": "Franky",
    "description": "tools in work",
    "version": "1.2.0-beta.0",
    "engines": {
        "vscode": "^1.83.0",
        "node": ">=18"
    },
    "publisher": "NorthSeacoder",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/NorthSeacoder/franky.git"
    },
    "icon": "images/tools.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "fileheader",
        "git log",
        "multi-root ready"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/index.js",
    "files": [
        "dist"
    ],
    "contributes": {
        "commands": [
            {
                "command": "franky.fileheader",
                "title": "fileheader",
                "category": "Franky"
            },
            {
                "command": "franky.diffLog",
                "title": "diffLog",
                "category": "Franky"
            },
            {
                "command": "franky.jenkins",
                "title": "跳转jenkins",
                "category": "Franky"
            },
            {
                "command": "franky.generate.vue",
                "title": "生成新页面(vue)",
                "category": "Franky"
            },
            {
                "command": "franky.generate.wp",
                "title": "生成新页面(wp)",
                "category": "Franky"
            },
            {
                "command": "franky.generate.react",
                "title": "生成新页面(react)",
                "category": "Franky"
            },
            {
                "command": "franky.generate.defs",
                "title": "根据选中内容生成 def",
                "category": "Franky"
            },
            {
                "command": "franky.generate.fields",
                "title": "根据选中内容生成 fields",
                "category": "Franky"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "submenu": "franky.explorerAction",
                    "group": "navigation@1"
                }
            ],
            "editor/context": [
                {
                    "submenu": "franky.editorAction",
                    "group": "navigation@1"
                }
            ],
            "franky.editorAction": [
                {
                    "command": "franky.generate.fields",
                    "group": "1_franky@2",
                    "when": "editorLangId == typescript || editorLangId == javascript || editorLangId == typescriptreact || editorLangId == javascriptreact"
                }
            ],
            "franky.explorerAction": [
                {
                    "command": "franky.generate.vue",
                    "group": "1_franky@1",
                    "when": "explorerResourceIsFolder"
                },
                {
                    "command": "franky.generate.react",
                    "group": "1_franky@2",
                    "when": "explorerResourceIsFolder"
                },
                {
                    "command": "franky.generate.wp",
                    "group": "1_franky@2",
                    "when": "explorerResourceIsFolder"
                }
            ]
        },
        "submenus": [
            {
                "id": "franky.explorerAction",
                "label": "Franky"
            },
            {
                "id": "franky.editorAction",
                "label": "Franky"
            }
        ],
        "keybindings": [
            {
                "command": "franky.fileheader",
                "key": "ctrl+alt+i",
                "when": "editorTextFocus"
            },
            {
                "command": "franky.diffLog",
                "key": "ctrl+alt+d",
                "when": "editorTextFocus"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "franky Configuration",
            "properties": {
                "franky.jenkins.url": {
                    "type": "string",
                    "default": "https://www.jenkins.io/",
                    "description": "your jenkins url,default value is jenkins's homepage"
                }
            }
        },
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/common.json"
            },
            {
                "language": "vue",
                "path": "./snippets/common.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/common.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/common.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/common.json"
            }
        ]
    },
    "scripts": {
        "cz": "git add . && git-cz",
        "cl": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "build": "tsup src/index.ts --external vscode --minify",
        "dev": "tsup src/index.ts --external vscode  --watch --sourcemap",
        "lint": "eslint .",
        "vscode:prepublish": "nr build",
        "publish": "vsce publish",
        "pack": "vsce package",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "release": "bumpp && nr publish"
    },
    "devDependencies": {
        "@nsea/cz": "0.0.2",
        "commitizen": "^4.1.5",
        "conventional-changelog": "^4.0.0",
        "conventional-changelog-cli": "^3.0.0",
        "@antfu/ni": "^0.21.9",
        "@types/node": "^20.9.0",
        "@types/vscode": "^1.83.0",
        "@vscode/vsce": "^2.22.0",
        "bumpp": "^9.2.0",
        "eslint": "^8.53.0",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2",
        "vitest": "^0.34.6",
        "dayjs": "^1.10.7",
        "simple-git": "^2.47.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {}
}
